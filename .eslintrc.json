{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "import/no-extraneous-dependencies": 1,
        "no-console": 0,
        "no-alert": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "no-dupe-else-if": "off",
        "indent": "off",
        "quote-props": [
            "error",
            "as-needed"
        ],
        "comma-dangle": [
            "error",
            "always-multiline"
        ],
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "no-shadow": [
            "error",
            {
                "allow": [
                    "reviewsData"
                ]
            }
        ],
        "no-restricted-syntax": "off",
        "guard-for-in": "off",
        "class-methods-use-this": "off",
        "no-undef": "off",
        "import/prefer-default-export": "off"
    }
}